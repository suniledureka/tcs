<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="studentHostel" class="com.tcs.spring.core.beans.Hostel" p:hostelName="Bangalore West Hostel" p:city="Bangalore" />
	
	<bean id="stud" class="com.tcs.spring.core.beans.Student" autowire="byName">
		<property name="studentId" value="857426"/>
		<property name="studentName" value="Praveen Kumar"/>		
	</bean>
</beans>
