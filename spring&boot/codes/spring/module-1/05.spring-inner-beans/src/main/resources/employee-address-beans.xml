<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="empl-1" class="com.tcs.spring.core.beans.Employee">
		<property name="employeeId" value="254126" />
		<property name="employeeName" value="Sunil Joseph" />
		<property name="employeeAddress">
			<bean class="com.tcs.spring.core.beans.Address">
				<property name="street" value="Ameerpet" />
				<property name="city" value="Hyderabad" />
				<property name="state" value="Telangana" />
			</bean>
		</property>
	</bean>
	
	<alias name="empl-1" alias="sunil"/>
</beans>
