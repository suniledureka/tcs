<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="stud1" class="com.tcs.spring.core.beans.Student" init-method="myInit1" destroy-method="myDestructor">
		<property name="name" value="Sanjay Patil"/>
	</bean>
	
	<bean id="stud2" class="com.tcs.spring.core.beans.Student" init-method="myInit2" destroy-method="myDestructor">
		<constructor-arg value="Praveen Kumar"/>
	</bean>	
</beans>
